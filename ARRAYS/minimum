import java.util.*;
public class Arr {
    public static void main(String[] args) {
        int a[] = {1, 2, 3, 4, 5};
        int minn=a[0];
        for(int i=0;i<a.length;i++){
                if(a[i]<minn){
                    minn=a[i];
                }
            }
        System.out.println(minn);
    }
}
